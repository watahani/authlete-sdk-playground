{
	"name": "TypeScript (Deprecated)",
	"dockerComposeFile": [
	  "./docker-compose.yml",
	  "../docker-compose-common.yml"
	],
	"initializeCommand": "bash .devcontainer/scripts/ensure-env.sh",
	"workspaceFolder": "/workspaces/${localWorkspaceFolderBasename}/typescript",
	"service": "worker",
	"remoteUser": "node",
	"updateRemoteUserUID": true,
	"features": {
	  "ghcr.io/devcontainers/features/node:1": {
		"version": "20"
	  }
	},
	"customizations": {
	  "vscode": {
		"extensions": [
		  "ms-vscode.vscode-typescript-next",
		  "dbaeumer.vscode-eslint"
		]
	  }
	}
  }
  
