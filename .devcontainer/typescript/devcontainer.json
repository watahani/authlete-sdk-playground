{
	"name": "TypeScript",
	"dockerComposeFile": [
	  "./docker-compose.yml"
	],
	"workspaceFolder": "../typescript",
	"service": "worker",
	"remoteUser": "vscode",
	"updateRemoteUserUID": true,
	"features": {
	  "ghcr.io/devcontainers/features/node:1": {
		"version": "20"
	  }
	},
	"customizations": {
	  "vscode": {
		"extensions": [
		  "ms-vscode.vscode-typescript-next",
		  "dbaeumer.vscode-eslint"
		]
	  }
	}
  }
  