# Copy this file to .env.local and fill in your Authlete credentials.

# Default base URL for v2 services
AUTHLETE_BASE_URL=https://api.authlete.com

# Service credentials (v2)
AUTHLETE_SERVICE_APIKEY=
AUTHLETE_SERVICE_APISECRET=

# Service access token (v3). When provided, AUTHLETE_SERVICE_APISECRET is optional.
AUTHLETE_SERVICE_ACCESSTOKEN=
AUTHLETE_API_VERSION=

# Optional service owner credentials
AUTHLETE_SERVICEOWNER_APIKEY=
AUTHLETE_SERVICEOWNER_APISECRET=
AUTHLETE_SERVICEOWNER_ACCESSTOKEN=

# Optional extras
AUTHLETE_DPOP_KEY=
AUTHLETE_CLIENT_CERTIFICATE=
